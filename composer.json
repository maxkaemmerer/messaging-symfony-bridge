{
    "name": "maxkaemmerer/messaging-symfony-bridge",
    "description": "This is a bridge providing DI for maxkaemmerer/events and maxkaemmerer/command in symfony.",
    "keywords": ["library", "composer", "package", "php", "events", "commands", "symfony", "bridge", "messaging"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Max Kaemmerer",
            "email": "maxkaemmerer.hb@googlemail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "maxkaemmerer/events": "^1.0",
        "maxkaemmerer/commands": "^1.0",
        "symfony/dependency-injection": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "MaxKaemmerer\\MessagingSymfonyBridge\\": "src"
        }
    }
}
